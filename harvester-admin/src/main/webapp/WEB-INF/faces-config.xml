<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
    <managed-bean>
        <managed-bean-name>resourceController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.JobController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>storageController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.StorageController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>transformationController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.TransformationController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>stepController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.StepController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>loginManager</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.LoginManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userTorusURI</property-name>
            <value>http://mk2.indexdata.com/torus2/identity.USERS/records/</value>
         </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>connectorItemSelectedListener</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.ConnectorItemSelectedListener</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>logged_out</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/jobs/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/jobs/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_resources</from-outcome>
            <to-view-id>/jobs/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_storages</from-outcome>
            <to-view-id>/storages/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_transformations</from-outcome>
            <to-view-id>/transformations/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_steps</from-outcome>
            <to-view-id>/steps/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>harvester_log</from-outcome>
            <to-view-id>/jobs/log.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>settings</from-outcome>
        <to-view-id>/settings/list.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>new_OaiPmhResource</from-outcome>
            <to-view-id>/jobs/add_oaipmh.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_XmlBulkResource</from-outcome>
            <to-view-id>/jobs/add_xmlbulk.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_WebCrawlResource</from-outcome>
            <to-view-id>/jobs/add_webcrawl.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_HarvestConnectorResource</from-outcome>
            <to-view-id>/jobs/add_connector.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_StatusResource</from-outcome>
            <to-view-id>/jobs/add_status.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_OaiPmhResource</from-outcome>
            <to-view-id>/jobs/edit_oaipmh.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_XmlBulkResource</from-outcome>
            <to-view-id>/jobs/edit_xmlbulk.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_WebCrawlResource</from-outcome>
            <to-view-id>/jobs/edit_webcrawl.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_HarvestConnectorResource</from-outcome>
            <to-view-id>/jobs/edit_connector.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_StatusResource</from-outcome>
            <to-view-id>/jobs/edit_status.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/storages/list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>new_solrstorage</from-outcome>
            <to-view-id>/storages/add_solr.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_zebrastorage</from-outcome>
            <to-view-id>/storages/add_zebra.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_foliostorage</from-outcome>
            <to-view-id>/storages/add_folio_storage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xmlstorage</from-outcome>
            <to-view-id>/storages/edit_xmlstorage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_consolestorage</from-outcome>
            <to-view-id>/storages/add_console.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_solrstorage</from-outcome>
            <to-view-id>/storages/edit_solr.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_foliostorage</from-outcome>
            <to-view-id>/storages/edit_folio_storage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_zebrastorage</from-outcome>
            <to-view-id>/storages/edit_zebra.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xmlstorage</from-outcome>
            <to-view-id>/storages/edit_xml.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_consolestorage</from-outcome>
            <to-view-id>/storages/edit_console.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/transformations/list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>new_transformation</from-outcome>
            <to-view-id>/transformations/add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_transformation</from-outcome>
            <to-view-id>/transformations/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/steps/list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>new_split_step</from-outcome>
            <to-view-id>/steps/split_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_split_step</from-outcome>
            <to-view-id>/steps/split_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_xslt_step</from-outcome>
            <to-view-id>/steps/xsl_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xslt_step</from-outcome>
            <to-view-id>/steps/xsl_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_xsd_step</from-outcome>
            <to-view-id>/steps/xsd_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xsd_step</from-outcome>
            <to-view-id>/steps/xsd_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_CustomTransformationStep</from-outcome>
            <to-view-id>/steps/custom_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_CustomTransformationStep</from-outcome>
            <to-view-id>/steps/custom_add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>delete_step</from-outcome>
            <to-view-id>/steps/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/transformations/add.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit_current</from-outcome>
            <to-view-id>/transformations/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/transformations/edit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>insert_step</from-outcome>
            <to-view-id>/transformations/insert_step.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>up_step</from-outcome>
            <to-view-id>/transformations/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>down_step</from-outcome>
            <to-view-id>/transformations/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>delete_step</from-outcome>
            <to-view-id>/transformations/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/transformations/insert_step.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit_current</from-outcome>
            <to-view-id>/transformations/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/transformations/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
