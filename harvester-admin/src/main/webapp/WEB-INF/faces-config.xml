<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>resourceController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.ResourceController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>storageController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.StorageController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>transformationController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.TransformationController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>stepController</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.StepController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>loginManager</managed-bean-name>
        <managed-bean-class>com.indexdata.masterkey.localindices.web.admin.controller.LoginManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userTorusURI</property-name>
            <value>http://mk2-test.indexdata.com/torus2/identity.USERS/records/</value>
         </managed-property>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/error.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>logged_out</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/list_resources.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/list_resources.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_resources</from-outcome>
            <to-view-id>/list_resources.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_storages</from-outcome>
            <to-view-id>/list_storages.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_transformations</from-outcome>
            <to-view-id>/list_transformations.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_steps</from-outcome>
            <to-view-id>/list_steps.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>harvester_log</from-outcome>
            <to-view-id>/harvester_log.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/list_resources.jsp</from-view-id>
        <navigation-case>
            <from-outcome>new_oaipmh</from-outcome>
            <to-view-id>/add_oaipmh.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_xmlbulk</from-outcome>
            <to-view-id>/add_xmlbulk.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_webcrawl</from-outcome>
            <to-view-id>/add_webcrawl.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_oaipmh</from-outcome>
            <to-view-id>/edit_oaipmh.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xmlbulk</from-outcome>
            <to-view-id>/edit_xmlbulk.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_webcrawl</from-outcome>
            <to-view-id>/edit_webcrawl.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/list_storages.jsp</from-view-id>
        <navigation-case>
            <from-outcome>new_solrstorage</from-outcome>
            <to-view-id>/add_solrstorage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_zebrastorage</from-outcome>
            <to-view-id>/add_zebra.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xmlstorage</from-outcome>
            <to-view-id>/edit_xmlstorage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_consolestorage</from-outcome>
            <to-view-id>/add_console.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_solrstorage</from-outcome>
            <to-view-id>/edit_solrstorage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_zebrastorage</from-outcome>
            <to-view-id>/edit_zebrastorage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xmlstorage</from-outcome>
            <to-view-id>/edit_xmlstorage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_consolestorage</from-outcome>
            <to-view-id>/edit_consolestorage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/list_transformations.jsp</from-view-id>
        <navigation-case>
            <from-outcome>new_transformation</from-outcome>
            <to-view-id>/transformation_add.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_transformation</from-outcome>
            <to-view-id>/transformation_edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/list_steps.jsp</from-view-id>
        <navigation-case>
            <from-outcome>new_split_step</from-outcome>
            <to-view-id>/step_split_add.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_split_step</from-outcome>
            <to-view-id>/step_split_add.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_xslt_step</from-outcome>
            <to-view-id>/step_xsl_add.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xslt_step</from-outcome>
            <to-view-id>/step_xsl_add.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_xsd_step</from-outcome>
            <to-view-id>/step_xsd_add.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit_xsd_step</from-outcome>
            <to-view-id>/step_xsd_add.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>delete_step</from-outcome>
            <to-view-id>/list_steps.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/transformation_edit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>insert_step</from-outcome>
            <to-view-id>/transformation_insert_step.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>up_step</from-outcome>
            <to-view-id>/transformation_edit.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>down_step</from-outcome>
            <to-view-id>/transformation_edit.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>delete_step</from-outcome>
            <to-view-id>/transformation_edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/transformation_insert_step.jsp</from-view-id>
        <navigation-case>
            <from-outcome>save</from-outcome>
            <to-view-id>/transformation_edit.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/list_transformations.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
